{
    "name": "genealabs/nova-telescope",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "laravel/framework": "^10.0",
        "laravel/telescope": "^4.13"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\NovaTelescope\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\NovaTelescope\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
